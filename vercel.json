{
    "version": 2,
    "builds": [
        {
            "src": "proyecto_clinica/wsgi.py",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/admin/(.*)",
            "dest": "proyecto_clinica/wsgi.py"
        },
        {
            "src": "/(.*)",
            "dest": "proyecto_clinica/wsgi.py"
        }
    ]
}